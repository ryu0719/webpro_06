<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>D.LEAGUE参画チーム</title>
</head>
<body>
  <h2>D.LEAGUE参画チーム一覧</h2>
  <table>
    <tr><th>ID</th><th></th></tr>
      <% let number = 0 %>
      <% for( let row of data ) { %>
        <tr><td><%= row.id %></td>
        <td><%= row.code %></td>
        <td><a href="/dleague/<%= number %>"><%= row.name %></a></td></tr>
        <% number += 1 %>
      <% } %>
  </table>
  <a href="/dleague/create">新チームの追加</a> 
</body>
</html>