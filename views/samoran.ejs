<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>サモラン</title>
</head>
<body>
  <h2>サーモンラン敵マニュアル</h2>
  <table>
    <tr><th>ID</th><th></th></tr>
      <% let number = 0 %>
      <% for( let row of data ) { %>
        <tr><td><%= row.id %></td>
        <td><%= row.code %></td>
        <td><a href="/samoran/<%= number %>"><%= row.name %></a></td></tr>
        <% number += 1 %>
      <% } %>
  </table>
</body>
</html>